--- 
import { ViewTransitions } from 'astro:transitions';
import '../themes/@global.scss'

interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Currinculum de un desarrollador Front-Web de Brandol Jesus Vargas">
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<meta property="og:title" content="Curriculum - Brandol Jesus Fontecha Vargas">
		<meta property="og:description" content="Currinculum de un desarrollador Front-Web de Brandol Jesus Vargas	">
		<meta property="og:url" content="https://buildsite.vercel.com">
		<link rel="preload" as="font" href="/fonts/Comfortaa-Regular.woff2" type="font/woff2" crossorigin="crossorigin">
		<link rel="preload" as="font" href="/fonts/Comfortaa-SemiBold.woff2" type="font/woff2" crossorigin="crossorigin">
		<link rel="preload" as="font" href="/fonts/Comfortaa-Bold.woff2" type="font/woff2" crossorigin="crossorigin">
		<title>{title}</title>
		<ViewTransitions></ViewTransitions>
	</head>
	<body >
		<slot />
	</body>
</html>
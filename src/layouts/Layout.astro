--- 
import '../themes/@global.scss'
import Noise from '@shared/content/Noise.astro'

interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
	<head>
		<title>{title}</title>
		<meta name="description" content="Currinculum de un desarrollador Front-Web de Brandol Jesus Vargas">
		<link rel="canonical" href="https://branjes-dev.vercel.app/">
		<meta charset="UTF-8" />

		<meta name="viewport" content="width=device-width" />
    <meta name="keywords" content="dev, developer, FrontWeb, FullStack, BackEnd, Curriculum, brandol" />
		
    <link rel="icon" type="image/svg+xml" href="/address-card.svg" />
    <meta name="msapplication-config" content="/browserconfig.xml" />

		<meta name="generator" content={Astro.generator} />
		<meta property="og:title" content="Curriculum - Brandol Jesus Fontecha Vargas">
		<meta property="og:description" content="Currinculum de un desarrollador Front-Web de Brandol Jesus Vargas	">
		<meta property="og:url" content="https://branjes-dev.vercel.app/">
    <meta name="og:site_name" content="Curriculum Brandol dev">
    <meta name="og:type" content="website">
    <meta name="og:locale" content="es_CO">

    <meta name="robots" content="index, follow" />
    <meta name="googlebot" content="index, follow" />
		<meta name="theme-color" content="#317EFB"/>

		<link rel="preload" href="/backgrounds/PerfilRemove.avif">
		<link rel="preload" href="/images/noise-50.avif">
		<link rel="preload" href="/fonts/Comfortaa-Regular.woff2" as="font" type="font/woff2" crossorigin="crossorigin">
		<link rel="preload" href="/fonts/Comfortaa-SemiBold.woff2" as="font" type="font/woff2" crossorigin="crossorigin">
		<link rel="preload" href="/fonts/Comfortaa-Bold.woff2" as="font" type="font/woff2" crossorigin="crossorigin">

    <link rel="manifest" href="/manifest.webmanifest" />

	</head>
	<body >
    <Noise/>
		<slot />
	</body>
</html>